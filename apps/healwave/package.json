{
  "name": "cosmic-hub-healwave",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "scripts": {
    "dev": "vite --port 3000 --host 0.0.0.0",
    "build": "vite build",
    "build:apps": "vite build",
    "build:analyze": "vite build --mode analyze",
    "lint": "ESLINT_USE_FLAT_CONFIG=true eslint . --cache --max-warnings=0",
    "lint:fix": "ESLINT_USE_FLAT_CONFIG=true eslint . --fix --max-warnings=0",
    "type-check": "tsc --noEmit",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "clean": "rm -rf dist .turbo node_modules/.cache"
  },
  "dependencies": {
    "@cosmichub/auth": "workspace:*",
    "@cosmichub/config": "workspace:*",
    "@cosmichub/frequency": "workspace:*",
    "@cosmichub/integrations": "workspace:*",
    "@cosmichub/subscriptions": "workspace:*",
    "@cosmichub/types": "workspace:*",
    "@cosmichub/ui": "workspace:*",
    "@eslint/js": "^8.57.1",
    "@radix-ui/react-slider": "^1.3.5",
    "@radix-ui/react-switch": "^1.2.5",
    "@radix-ui/react-tooltip": "^1.2.7",
    "@radix-ui/react-tabs": "^1.0.5",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "firebase": "^10.12.2",
    "globals": "^13.24.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.30.1",
    "react-icons": "^5.2.1"
  },
  "devDependencies": {
    "@types/react": "^18.3.0",
    "@types/react-dom": "^18.3.0",
    "@types/node": "^20.14.10",
    "@testing-library/react": "^16.0.1",
    "@testing-library/user-event": "^14.5.2",
    "@typescript-eslint/eslint-plugin": "^7.15.0",
    "@typescript-eslint/parser": "^7.15.0",
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.19",
    "eslint": "^8.57.0",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.7",
    "postcss": "^8.4.38",
    "tailwindcss": "^3.4.4",
    "typescript": "^5.2.2",
    "vite": "^5.4.19",
    "vitest": "^2.1.4",
    "@vitest/ui": "^2.1.4",
    "@vitest/coverage-v8": "^2.1.4"
  }
