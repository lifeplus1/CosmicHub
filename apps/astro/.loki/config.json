  "storiesFilter": ".*--sample$|.*--basic$",
  "chromeFlags": ["--disable-gpu", "--no-sandbox"],
  "diffingEngine": "pixelmatch",
  "outputDir": "./.loki",
  "storybookBuildDir": "storyboard-static"
}
