{
  "name": "@cosmichub/config",
  "version": "1.0.0",
  "description": "Shared configuration for CosmicHub applications",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./api-result": {
      "types": "./dist/api-result.d.ts",
      "default": "./dist/api-result.js"
    },
    "./push-notifications": {
      "types": "./dist/push-notifications.d.ts",
      "default": "./dist/push-notifications.js"
    },
    "./firebase": {
      "types": "./dist/firebase.d.ts",
      "default": "./dist/firebase.js"
    },
    "./enhanced-testing": {
      "types": "./dist/enhanced-testing.d.ts",
      "default": "./dist/enhanced-testing.js"
    },
    "./accessibility-testing": {
      "types": "./dist/accessibility-testing.d.ts",
      "default": "./dist/accessibility-testing.js"
    },
    "./component-architecture": {
      "types": "./dist/component-library.d.ts",
      "default": "./dist/component-library.js"
    },
    "./component-library": {
      "types": "./dist/component-library.d.ts",
      "default": "./dist/component-library.js"
    },
    "./performance": {
      "types": "./dist/performance.d.ts",
      "default": "./dist/performance.js"
    },
    "./lazy-loading": {
      "types": "./dist/lazy-loading.d.ts",
      "default": "./dist/lazy-loading.js"
    },
    "./hooks": {
      "types": "./dist/hooks/index.d.ts",
      "default": "./dist/hooks/index.js"
    }
  },
  "scripts": {
    "build": "tsc -p tsconfig.build.runtime.json",
    "build:types": "tsc -p tsconfig.build.json --emitDeclarationOnly",
    "build:packages": "tsc -p tsconfig.build.json",
    "dev": "tsc --watch",
    "type-check": "tsc --noEmit",
    "test": "vitest",
    "test:run": "vitest run",
    "test:watch": "vitest watch",
    "test:coverage": "vitest run --coverage"
  },
  "dependencies": {
    "zod": "^3.22.0"
  },
  "devDependencies": {
    "typescript": "^5.0.0",
    "@types/node": "^20.0.0",
    "@testing-library/jest-dom": "^6.0.0",
    "@testing-library/react": "^14.0.0",
    "@vitejs/plugin-react": "^4.0.0",
    "jsdom": "^25.0.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "vitest": "^2.1.4",
    "@vitest/ui": "^2.1.4",
    "@vitest/coverage-v8": "^2.1.4"
  },
  "files": [
    "dist",
    "src"
  ]
}
