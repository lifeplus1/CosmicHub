{
  "name": "@cosmichub/config",
  "version": "1.0.0",
  "description": "Shared configuration for CosmicHub applications",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
  ".": "./src/index.ts",
  "./api-result": "./src/utils/api/result.ts",
  "./push-notifications": "./src/push-notifications.ts",
    "./firebase": "./src/firebase.ts",
    "./enhanced-testing": "./src/enhanced-testing.tsx",
    "./accessibility-testing": "./src/accessibility-testing.tsx",
    "./component-architecture": "./src/component-library.tsx",
    "./performance": "./src/performance.ts",
    "./lazy-loading": "./src/lazy-loading.tsx",
    "./hooks": "./src/hooks/index.ts"
  },
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "build:types": "tsc -p tsconfig.json --emitDeclarationOnly",
    "build:packages": "tsc",
    "dev": "tsc --watch",
    "type-check": "tsc --noEmit",
    "test": "vitest",
    "test:run": "vitest run",
    "test:watch": "vitest watch",
    "test:coverage": "vitest run --coverage"
  },
  "dependencies": {
    "zod": "^3.22.0"
  },
  "devDependencies": {
    "typescript": "^5.0.0",
    "@types/node": "^20.0.0",
    "@testing-library/jest-dom": "^6.0.0",
    "@testing-library/react": "^14.0.0",
    "@vitejs/plugin-react": "^4.0.0",
  "jsdom": "^25.0.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
  "vitest": "^2.1.4",
  "@vitest/ui": "^2.1.4",
  "@vitest/coverage-v8": "^2.1.4"
  },
  "files": [
    "dist",
    "src"
  ]
}
